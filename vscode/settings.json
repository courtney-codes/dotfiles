{
  "editor.autoIndent": "full",
  "editor.detectIndentation": true,
  "editor.fontFamily": "Cascadia Mono PL",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.formatOnSave": false,
  "editor.linkedEditing": true,
  "window.restoreFullscreen": true,
  "editor.minimap.enabled": false,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "explorer.openEditors.visible": 0,
  "emmet.includeLanguages": {
    "vue-html": "html",
    "vue": "html",
    "django-html": "html",
  },
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "emmet.syntaxProfiles": {
    "html": {
      "compact_bool": true,
      "self_closing_tag": "xhtml"
    }
  },
  "prettier.printWidth": 120,
  "prettier.singleQuote": true,
  "[javascript]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[c]": {
    "editor.wordBasedSuggestions": false,
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autofetch": true,
  "git.inputValidationSubjectLength": 72,
  "git.confirmSync": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "git.enableSmartCommit": true,
  "git.promptToSaveFilesBeforeCommit": "staged",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "eslint.validate": ["javascript", "vue"],
  "workbench.settings.editor": "json",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "breadcrumbs.enabled": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "editor.renderWhitespace": "none",
  "editor.tabSize": 2,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Snazzy Plus",
  "diffEditor.ignoreTrimWhitespace": false,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[python]": {
    "gitlens.codeLens.symbolScopes": [
      "!Module"
    ],
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "editor.defaultFormatter": "ms-python.python",
    "editor.wordBasedSuggestions": false
  },
  "python.defaultInterpreterPath": "/Users/courtneysenior/.local/share/virtualenvs/patch-kpf28yRX/bin/python",
  "python.linting.lintOnSave": true,  
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": [
    "--load-plugins",
    "pylint_django",
    "pylint_celery"
  ],
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Path": "/Users/courtneysenior/.local/share/virtualenvs/dev-SXHolp3N/bin/autopep8",
  "python.formatting.autopep8Args": [
    "--max-line-length=88"
  ],
  "git.ignoreRebaseWarning": true,
  "workbench.editor.untitled.hint": "hidden",
  "vsicons.dontShowNewVersionMessage": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "remote.SSH.defaultExtensions": [
    "gitpod.gitpod-remote-ssh"
  ],
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
